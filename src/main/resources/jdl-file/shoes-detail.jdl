entity ShoesDetails {
    code String,
    price BigDecimal,
    quantity Long,
    status Integer,
    createdBy String,
    createdDate Instant,
    lastModifiedBy String,
    lastModifiedDate Instant
}
entity Shoes
entity Brand

relationship ManyToOne {
    ShoesDetails{shoes} to Shoes
}
relationship ManyToOne {
    ShoesDetails{brand} to Brand
}
service *  with serviceImpl
dto all with mapstruct
paginate * with pagination
