<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CartDetails.
    -->
    <changeSet id="20230918170619-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="cart_id"
                                 baseTableName="cart_details"
                                 constraintName="fk_cart_details__cart_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cart"/>

        <addForeignKeyConstraint baseColumnNames="shoes_details_id"
                                 baseTableName="cart_details"
                                 constraintName="fk_cart_details__shoes_details_id"
                                 referencedColumnNames="id"
                                 referencedTableName="shoes_details"/>
    </changeSet>
</databaseChangeLog>
