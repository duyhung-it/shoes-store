<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity ShoesDetails.
    -->
    <changeSet id="20230918164720-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="shoes_id"
                                 baseTableName="shoes_details"
                                 constraintName="fk_shoes_details__shoes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="shoes"/>

        <addForeignKeyConstraint baseColumnNames="brand_id"
                                 baseTableName="shoes_details"
                                 constraintName="fk_shoes_details__brand_id"
                                 referencedColumnNames="id"
                                 referencedTableName="brand"/>
    </changeSet>
</databaseChangeLog>
